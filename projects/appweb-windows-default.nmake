#
#   uninstall
#
DEPS_96 = $(DEPS_96) build
DEPS_96 = $(DEPS_96) compile
DEPS_96 = $(DEPS_96) stop

uninstall: $(DEPS_96)
	cd package
	if exist "$(ME_ETC_PREFIX)\appweb.conf" del /Q "$(ME_ETC_PREFIX)\appweb.conf"
	if exist "$(ME_ETC_PREFIX)\esp.conf" del /Q "$(ME_ETC_PREFIX)\esp.conf"
	if exist "$(ME_ETC_PREFIX)\mine.types" del /Q "$(ME_ETC_PREFIX)\mine.types"
	if exist "$(ME_ETC_PREFIX)\install.conf" del /Q "$(ME_ETC_PREFIX)\install.conf"
	if exist "$(ME_INC_PREFIX)\appweb" rd /Q /S "$(ME_INC_PREFIX)\appweb"
	if exist "$(ME_WEB_PREFIX)" rd /Q /S "$(ME_WEB_PREFIX)"
	if exist "$(ME_SPOOL_PREFIX)" rd /Q /S "$(ME_SPOOL_PREFIX)"
	if exist "$(ME_CACHE_PREFIX)" rd /Q /S "$(ME_CACHE_PREFIX)"
	if exist "$(ME_LOG_PREFIX)" rd /Q /S "$(ME_LOG_PREFIX)"
	if exist "$(ME_VAPP_PREFIX)" rd /Q /S "$(ME_VAPP_PREFIX)"
	if exist "$(ME_APP_PREFIX)\latest" del /Q "$(ME_APP_PREFIX)\latest"
	if exist "$(ME_APP_PREFIX)" rd /Q "$(ME_APP_PREFIX)"
	cd ..

#
#   version
#
version: $(DEPS_97)
#  Omit build script version

