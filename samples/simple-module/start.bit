/*
	start.bit - Bit file for the simple-module sample
 */
Bit.load({
    settings: {
        requires: ['appweb'],
        version: '1.0.0',
    },
    targets: {
		libmod_simple: {
			type: 'lib',
			sources: ['simple.c'],
            depends: ['appweb'],
		}
        server: {
            type:    'exe',
            sources: 'server.c',
            depends: ['appweb'],
        },
        run: {
            run: './server',
        },
    },
})
