/*
    start.bit - Bit file for the simple-esp sample
 */

Bit.load({
    settings: {
        requires: ['appweb'],
    },
    targets: {
        server: {
            type:    'exe',
            sources: '*.c',
            depends: ['appweb'],
        },
        run: {
            run: 'sudo ./server',
        },
    },
})
