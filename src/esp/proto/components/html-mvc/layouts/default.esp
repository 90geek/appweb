<!DOCTYPE html>
<html lang="en">
<head>
    <title>${TITLE}</title>
    <meta charset="utf-8" />
    <link rel="icon" href="/assets/favicon.ico" type="image/x-icon">
    <% stylesheets(NULL); %>
    <% securityToken(); %>
</head>
<body id='body' data-config='<% renderConfig(); %>'>
    <nav class="navbar navbar-inverse navbar-static-top">
        <div class="navbar-header">
            <a class="navbar-brand" href="#">${TITLE}</a>
        </div>
    </nav>
    <main class="container">
        <section class="content col-sm-10" id="content">
            <%@ content %>
        </section>
        <div class="feedback">
        <% { 
            MprKey  *kp;
            EspReq  *req = getConn()->data; 
            if (req->flash) for (ITERATE_KEYS(req->flash, kp)) {
                render("<span>%s</span>", kp->data);
            }
        } %>
        </div>
    </main>
    <% scripts(NULL); %>
</body>
</html>
