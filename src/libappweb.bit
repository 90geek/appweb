/*
    libappweb.bit -- Bit File for the Appweb library
 */

Bit.load({

    defaults: {
        '+includes': [ '.' ]
    },

    targets: {
        libappweb: {
            type: 'lib',
            sources: [ '*.c' ],
            headers: [ '*.h' ],
            depends: [ 'libmpr', 'libhttp', 'libpcre', 'libmprssl' ],
        },
	},
})
